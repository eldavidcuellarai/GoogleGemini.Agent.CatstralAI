# Google Gemini API Configuration
# Obtain your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
GOOGLE_GENERATIVE_AI_API_KEY=your_gemini_api_key_here

# Application Configuration
NODE_ENV=development
PORT=3000

# Next.js Application (v0-registro-de-la-propiedad)
NEXT_PUBLIC_APP_NAME="Catastral Registry Analyzer"
NEXT_PUBLIC_APP_VERSION="1.0.0"

# Simple HTML Interface (el cerebro de gemini)
DEFAULT_PROCESSING_MODE=sequential
MAX_FILES=10
MAX_TOTAL_SIZE_MB=50
MAX_SINGLE_FILE_MB=20

# Gemini API Configuration
GEMINI_MODEL=gemini-2.0-flash-exp
GEMINI_FALLBACK_MODEL=gemini-1.5-flash
GEMINI_API_TEMPERATURE=0.3
GEMINI_API_TOP_P=0.8
GEMINI_API_TOP_K=40
GEMINI_MAX_OUTPUT_TOKENS=8192

# UI Configuration
DEFAULT_THEME=light
ENABLE_DARK_MODE=true
ENABLE_LOCAL_STORAGE=true
ENABLE_FILE_VALIDATION=true

# PDF.js Configuration
PDFJS_WORKER_URL=https://cdnjs.cloudflare.com/ajax/libs/pdf.js/3.11.174/pdf.worker.min.js

# Security Configuration
ENABLE_API_KEY_MASKING=true
LOG_LEVEL=info

# Catastral API Configuration (Optional)
CATASTRAL_API_ENABLED=false
CATASTRAL_API_URL=https://your-catastral-api.com/extract